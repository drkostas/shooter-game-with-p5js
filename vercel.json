{
    "functions": {
      "api/*.php": {
        "runtime": "vercel-php@0.6.0"
      }
    },
    "routes": [
      { "src": "/api/submitScore", "dest": "/api/submitScore.php" },
      { "src": "/api/(.*)", "dest": "/api/index.php" },
      { "src": "/", "dest": "/api/index.php" },
      { "src": "/(.*)", "dest": "/$1" }
    ]
  }
  