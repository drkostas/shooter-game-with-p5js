{
    "functions": {
      "api/*.php": {
        "runtime": "vercel-php@0.6.0"
      }
    },
    "routes": [
      { "src": "/api/submit_score", "dest": "/api/submit_score.php" },
      { "src": "/api/create_table_track", "dest": "/api/create_table_track.php" },
      { "src": "/api/(.*)", "dest": "/api/index.php" },
      { "src": "/", "dest": "/api/index.php" },
      { "src": "/(.*)", "dest": "/$1" }
    ]
  }
  